{% extends "page.twig" %}



{% block body %}

{% for table in tables %}
	{% if table.title %}
		<h3>{{ table.title }}</h3>
	{% endif %}
	<table class="table">
		{% if table.headers %}
			<thead>
				<tr>
					{% for header in table.headers %}
						<th>{{ header }}</th>
					{% endfor %}
				</tr>
			</thead>
		{% endif %}
		<tbody>
			{% for row in table.data %}
				<tr>
					{% for value in row %}
						{% if value is not iterable %}
						<td>{{ value }}</td>
						{% endif %}
					{% endfor %}
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endfor %}


{% endblock %}
